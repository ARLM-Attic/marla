

type Lit =
	| StringLit value:String
	| IntLit value:Int
	| IntLit value:Int
	
	
type NamedType =
	name: String
	
type NamesPassResult =
	names: { String: NamedType }

type FrontEnd 'ir 'irNames 'irTypes =
	parse: (doc: Document): 'ir
	namesPass: (doc: Document, ir: 'ir): (NamesPassResult, 'irNames)
	typesPass: (doc: Document, ir: 'ir, irNames: 'irNames): (TypesPassResult, 'irTypes)
	astPass: (doc: Document, ir: 'ir, irNames: 'irNames, irTypes: 'irTypes): AstPassResult
	


